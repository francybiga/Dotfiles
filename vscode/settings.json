{
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": "#548a04",
    "vim.statusBarColors.insert": "#195478",
    "vim.statusBarColors.visual": "#ffaf01",
    "vim.statusBarColors.visualblock": "#ffaf01",
    "vim.statusBarColors.replace": "#c53019",
    "workbench.colorCustomizations": {
        "statusBar.background": "#ffaf01",
        "statusBar.noFolderBackground": "#ffaf01",
        "statusBar.debuggingBackground": "#ffaf01"
    },
    "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
    "editor.renderWhitespace": "all",
    "vim.useSystemClipboard": true,
    "files.autoSave": "afterDelay",
    "security.workspace.trust.untrustedFiles": "open",
    "git.openRepositoryInParentFolders": "always",
    "[python]": {
        "editor.formatOnType": true
    },
    "window.zoomLevel": 3,
    "editor.find.seedSearchStringFromSelection": "never",
}